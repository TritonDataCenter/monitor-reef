  "MANTA-5480": {
    "key": "MANTA-5480",
    "id": "10005",
    "fields": {
      "summary": "Objects are not garbage-collected when replaced",
      "status": {
        "name": "Open"
      },
      "resolution": null,
      "created": "2025-07-14T09:31:28.857-0400",
      "updated": "2025-09-15T16:16:30.435-0400",
      "labels": ["public"],
      "description": {
        "type": "doc",
        "version": 1,
        "content": [
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "text": "When an object in Manta is overwritten with a new version, the old version's storage is not reclaimed by the garbage collector."
              }
            ]
          }
        ]
      },
      "comment": {
        "comments": []
      }
    },
    "renderedFields": {
      "description": "<p>When an object in Manta is overwritten with a new version, the old version's storage is not reclaimed by the garbage collector.</p>"
    }
  }
