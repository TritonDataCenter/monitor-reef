  "OS-8638": {
    "key": "OS-8638",
    "id": "10003",
    "fields": {
      "summary": "Want ability to specify zfs_arc_max",
      "status": {
        "name": "Open"
      },
      "resolution": null,
      "created": "2025-03-24T14:44:42.319-0400",
      "updated": "2025-12-01T09:36:49.571-0500",
      "labels": ["public"],
      "description": {
        "type": "doc",
        "version": 1,
        "content": [
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "text": "Currently there is no supported way to set zfs_arc_max on SmartOS. This is needed for systems with large amounts of RAM where the default ARC sizing is not optimal."
              }
            ]
          }
        ]
      },
      "comment": {
        "comments": []
      }
    },
    "renderedFields": {
      "description": "<p>Currently there is no supported way to set zfs_arc_max on SmartOS. This is needed for systems with large amounts of RAM where the default ARC sizing is not optimal.</p>"
    }
  },
